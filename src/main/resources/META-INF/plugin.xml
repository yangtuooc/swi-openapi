<idea-plugin>
  <id>cn.haloop.swi.apifox</id>
  <name>swi-apifox-helper</name>
  <vendor email="mawh@bxlsj.com" url="https://www.haloop.cn">StarWish Tech</vendor>
  <description>
    Go ApiFox插件支持
    支持框架：Gin
    无需ApiFox注释生成文档，通过Json Schema的方式导入ApiFox
  </description>
  <version>1.0.0</version>

  <depends>com.intellij.modules.platform</depends>
  <depends>org.jetbrains.plugins.go</depends>
  <depends>com.intellij.modules.json</depends>

  <extensions defaultExtensionNs="com.intellij">
    <codeInsight.lineMarkerProvider
      language="go"
      implementationClass="cn.haloop.swi.helper.providers.SwiLineMarkerProvider"/>
  </extensions>
  <actions>
  </actions>
</idea-plugin>